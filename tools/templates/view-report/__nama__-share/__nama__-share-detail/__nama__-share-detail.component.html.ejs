                    [flow-id]="<%= nama_object %>.id_data_flow" [flow-detail]="<%= nama_object %>.id_data_flow_detail"
                    (reload)="approve($event)">
                </approval-mode-widget> -->

            </div>
        </div>


        <br>

 
    </div>

    <div class="site-page-header-ghost-wrapper">
        <nz-page-header [nzGhost]="false">
        <nz-page-header-title>Item</nz-page-header-title>
    </nz-page-header>
    </div >

    <div class="card">
        <div class="card-body p-2">
                <!-- <app-<%= nama %>-share-list [filter-extra]="false" [id<%= Nama %>]="id<%= Nama %>"></app-<%= nama %>-share-list> -->
    </div>
