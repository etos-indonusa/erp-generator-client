        <div class="row">
            <div class="col-md-8">
                <app-<%= nama %>-share-header [data]="<%= nama_object %>"></app-<%= nama %>-share-header>
            </div>
            <div class="col-md-4">
                <document-widget-v1 [for-module-id]="<%= nama_object %>?.id<%= Nama %>" [for-module]="'contract_site'"  [enable-crud]="true"></document-widget-v1>
                    
                <!-- <approval-mode-widget *ngIf="<%= nama_object %>.status??'-'.toLowerCase() !='close'"
